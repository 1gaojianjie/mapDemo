<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gcc.gccmap.mapper.UserMapper">
    <update id="updateUser" parameterType="com.gcc.gccmap.model.vo.ShowUserInfo">
        update gccmap.user set user_name = #{userName},phone_num = #{phoneNum}
        where user_id = #{userId}
    </update>
</mapper>
