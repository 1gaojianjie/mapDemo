<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gcc.gccmap.mapper.UserAndRoleMapper">
    <select id="queryRoleIdByUID" resultType="int">
        select role_id from user_role
        where u_id = #{userId}
    </select>

</mapper>